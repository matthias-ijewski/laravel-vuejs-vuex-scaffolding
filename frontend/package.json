{
    "name": "frontend",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint"
    },
    "dependencies": {
        "axios": "^0.19.0",
        "bootstrap": "^4.4.1",
        "bootstrap-vue": "^2.1.0",
        "core-js": "^3.1.2",
        "tiptap": "^1.26.5",
        "tiptap-extensions": "^1.28.5",
        "vue": "^2.6.10",
        "vue-moment": "^4.0.0",
        "vue-router": "^3.0.6",
        "vuedraggable": "^2.23.2",
        "vuelidate": "^0.7.4",
        "vuetify": "^2.1.0",
        "vuetify-confirm": "^2.0.2",
        "vuex": "^3.0.1"
    },
    "devDependencies": {
        "@mdi/font": "^4.5.95",
        "@vue/cli-plugin-babel": "^4.0.0",
        "@vue/cli-plugin-eslint": "^4.0.0",
        "@vue/cli-service": "^4.0.0",
        "@vue/eslint-config-prettier": "^5.0.0",
        "babel-eslint": "^10.0.1",
        "eslint": "^5.16.0",
        "eslint-plugin-prettier": "^3.1.0",
        "eslint-plugin-vue": "^5.0.0",
        "prettier": "^1.18.2",
        "sass": "^1.19.0",
        "sass-loader": "^8.0.0",
        "vue-cli-plugin-vuetify": "^1.1.0",
        "vue-template-compiler": "^2.6.10",
        "vuetify-loader": "^1.3.0"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "@vue/prettier"
        ],
        "rules": {
            "no-console": "off",
            "no-debugger": "off",
            "eslint-disable-next-line": "off",
            "no-unused-vars": "off",
            "no-unused-components": "off"
        },
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "prettier": {
        "tabWidth": 4,
        "singleQuote": true
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ]
}
