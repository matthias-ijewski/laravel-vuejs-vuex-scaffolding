        "lint": "vue-cli-service lint"
    },
    "dependencies": {
        "axios": "^0.19.2",
        "bootstrap": "^4.5.0",
        "bootstrap-vue": "^2.15.0",
        "core-js": "^3.6.5",
        "tiptap": "^1.27.1",
        "tiptap-extensions": "^1.29.1",
        "vue": "^2.6.11",
        "vue-moment": "^4.1.0",
        "vue-router": "^3.3.4",
        "vuedraggable": "^2.23.2",
        "vuelidate": "^0.7.5",
        "vuetify": "^2.3.3",
        "vuetify-confirm": "^2.0.5",
        "vuex": "^3.5.1"
    },
    "devDependencies": {
        "@mdi/font": "^5.3.45",
        "@vue/cli-plugin-babel": "^4.4.6",
        "@vue/cli-plugin-eslint": "^4.4.6",
        "@vue/cli-service": "^4.4.6",
        "@vue/eslint-config-prettier": "^6.0.0",
        "babel-eslint": "^10.1.0",
        "eslint": "^7.4.0",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-vue": "^6.2.2",
        "prettier": "^2.0.5",
        "sass": "^1.26.9",
        "sass-loader": "^9.0.1",
        "vue-cli-plugin-vuetify": "^2.0.6",
        "vue-template-compiler": "^2.6.11",
        "vuetify-loader": "^1.5.0"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "@vue/prettier"
        ],
        "rules": {
            "no-console": "off",
            "no-debugger": "off",
            "eslint-disable-next-line": "off",
            "no-unused-vars": "off",
            "no-unused-components": "off"
        },
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "prettier": {
        "tabWidth": 4,
        "singleQuote": true
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ]
}
